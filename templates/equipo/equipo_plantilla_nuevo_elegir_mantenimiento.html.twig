{#EQUIPO PLANTILLA ELEGIR MANTENIMIENTO#}

{#MACROS#}
{% import "form.html.twig" as form %}

<div id="page-wrapper">
  <div class="row">
    <div class="col-lg-12">
      <h1 class="page-header">{{tituloModulo}}</h1>{#TÍTULO DEL MÓDULO#}
    </div>{#/.col-lg-12#}
  </div>{#/.row#}
  <div class="row">
    <div class="col-lg-12">
      <div class="panel panel-default">
        <div class="panel-heading">
          {{tituloPanel}}{#TÍTULO DEL PANEL#}
        </div>
        <div class="panel-body">
          <div class="row">
            <div class="col-lg-6">
                {# NOMBRE #}
                <form role="form" action="{{formAction }}" method="post">
                {{ form.inputText(txtNombreEquipoLabel, txtNombreEquipoName, txtNombreEquipoValue,
                    txtNombreEquipoEditar, txtNombreEquipoHelpBlock) }}
                {#SELECT SO#}
                {{ form.selectSO() }}
                <button type="submit" class="btn btn-primary">Crear</button>
              </form>
            </div>{#/.col-lg-6#}
          </div>{#/.row (nested)#}
        </div>{#/.panel-body#}
      </div>{#/.panel#}
    </div>{#/.col-lg-12#}
  </div>{#/.row#}
</div>{#/.page-wrapper#}
